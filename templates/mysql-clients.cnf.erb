#
# These groups are read by Percona command-line tools
# Use it for options that affect only one utility
#
[mysql]
prompt = "\u@\h [DB: \d]> "
<%= @custom_client_cnf_parameters %>

[mysql_upgrade]

[mysqladmin]

[mysqlbinlog]

[mysqlcheck]

[mysqldump]
max-allowed-packet = 512M

[mysqlimport]

[mysqlshow]

[mysqlslap]
